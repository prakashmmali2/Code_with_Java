import java.util.*;
public class Main
{
    static void fiboSeries(int n,int fir ,int sec){
        if(n>0){
                System.out.print(fir+ " ");
                fiboSeries(n-1,sec , fir + sec);
        }
    }
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the Ending Number : ");
		int n = sc.nextInt();
		int fir = 0;
		int sec = 1;
		System.out.print("Fibonacci Series given Number value : ");
		fiboSeries(n,fir,sec);
	}
}


